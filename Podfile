platform :ios, '8.0'

inhibit_all_warnings!
source 'https://github.com/CocoaPods/Specs.git'

def marcoPods
    pod 'Masonry'
    pod 'UINavigation-SXFixSpace'
#    pod 'AFNetworking'
    pod 'MBProgressHUD'
    pod 'ReactiveObjC'
    pod 'MJRefresh'
    pod 'SVProgressHUD', '~> 2.2.5'
    pod 'BRPickerView'
end


target 'HTPlace' do
    marcoPods
end


